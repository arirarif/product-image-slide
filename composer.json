{
    "name": "arirarif/product-image-auto-slider",
    "description": "Automatically slides WooCommerce product gallery images on single product pages",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "AriRarif",
            "homepage": "https://github.com/arirarif"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "yoast/phpunit-polyfills": "^1.0",
        "mockery/mockery": "^1.4",
        "brain/monkey": "^2.6"
    },
    "autoload": {
        "classmap": [
            "product-image-auto-slider.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PIAS\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage"
    }
}
